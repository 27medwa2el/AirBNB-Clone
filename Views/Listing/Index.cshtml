@model AirBNB.Controllers.CreatePropertyViewModel
@{
	Layout = null;
}
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Profile</title>
    <!-- Link to Bootstrap CSS file -->
    <link rel="stylesheet" href="./listing/bootstrap.css">
    <!-- Link to CSS files -->
    <link href="https://atlas.microsoft.com/sdk/javascript/mapcontrol/2/atlas.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="./listing/style.css">

</head>

<body>
    <!-- Navigation Bar -->
    
    <nav class="navbar navbar-expand-lg bg-white sticky-top  " style="height: 80px;">
        <!-- Start div tha t contain the items in nav -->
        <div class="container-lg   d-flex flex-row justify-content-between ">
            <!-- Start Logo -->
            <div class="d-flex align-items-center ">
                <img style="width: 32px; height: 32px;" src="./listing/SVG/logo.svg">
            </div>
            <!-- End Logo -->
            <!-- Start person -->
            <div>
                <div class="d-flex justify-content-between align-items-center">
                    <button class="btn text-decoration-none rounded-pill"
                            style="   box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.1);">
                        Exit
                    </button>
                </div>
                <!-- End person -->
            </div>
        </div>
        <!-- End div tha t contain the items in nav -->
    </nav>
    <!-- slider -->
    <form method="post" enctype="multipart/form-data">
        
    <div class="slider-container">
        <!-- step 1 -->
        <div class="step-container container">
            <section class="h-75 d-flex align-self-lg-center ">
                <div class="row">
                    <div class="col-1"></div>
                    <div class="col-md-6 mt-5 d-flex align-content-center flex-column justify-content-center">
                        <p class="fw-semibold fs-4">Step 1</p>
                        <h1>
                            Tell us about your place
                        </h1>
                        <div class="fw-normal fs-6">
                            In this step, we'll ask you which type of property you have and if guests will book the entire place or
                            just a room. Then let us know the location and how many guests can stay.
                        </div>
                    </div>
                    <div class="col-md-5 mt-5">

                        <video class="h-75 align-self-lg-center" autoplay>
                            <source src="./listing/Step 1- Tell us about your place.mp4" type="video/mp4">
                        </video>
                    </div>
                </div>
            </section>

            <button type="button" class="btn btn-dark float-end step-next-btn">Next</button>
        </div>
        <div class="step-container d-none">
           
            <div class="col-md-7 m-auto">
                <div class="p-2 mb-3">
                    <h1 class="text-center">What type of place will guests have?</h1>
                </div>
               
                <div class="d-grid gap-2 m-1">
                        <input  type="radio" class="btn-check" name="Catogery" id="1" autocomplete="off" value="An apartment">
                    <label class="btn  btn-outline-secondary" for="1">
                        <div class="d-flex mb-3">
                            <div class="me-auto p-2 text-start">
                                <h2>An apartment</h2>
                                <p>Guests have the whole place to themselves</p>
                            </div>
                            <div class="p-2 d-flex flex-column">
                                <img src="https://a0.muscache.com/pictures/64b27fed-56a1-4f03-950a-d8da08efb428.jpg"
                                     class="card-img-top cardimg rounded-circle"
                                     alt="Property 2" />
                            </div>
                        </div>
                    </label>
                </div>

                <div class="d-grid gap-2 m-1">
                    <input type="radio" class="btn-check" name="Catogery" id="2" autocomplete="off" value="An Villa">
                    <label class="btn btn-outline-secondary" for="2">
                        <div class="d-flex mb-3">
                            <div class="me-auto p-2 text-start">
                                <h2>An Villa</h2>
                                <p>Guests have the whole place to themselves</p>
                            </div>

                            <div class="p-2 d-flex flex-column">
                                <img src="https://a0.muscache.com/pictures/4d7580e1-4ab2-4d26-a3d6-97f9555ba8f9.jpg"
                                     class="card-img-top cardimg"
                                     alt="Property 1" />
                            </div>
                        </div>
                    </label>

                </div>
                <div class="d-grid gap-2 m-1">
                        <input type="radio" class="btn-check" name="Catogery" id="3" autocomplete="off" value="A Room">
                    <label class="btn btn-outline-secondary" for="3">
                        <div class="d-flex mb-3">
                            <div class="me-auto p-2 text-start">
                                <h2>A Room</h2>
                                <p>Guests have the whole place to themselves</p>
                            </div>

                            <div class="p-2 d-flex flex-column">
                                <img src="https://a0.muscache.com/pictures/4d7580e1-4ab2-4d26-a3d6-97f9555ba8f9.jpg"
                                     class="card-img-top cardimg"
                                     alt="Property 1" />
                            </div>
                        </div>

                </div>
                </label>
              
            </div>

            <div class="d-flex justify-content-between container flex-row-reverse pb-5">
                    <button type="button" name="catId" id="categorycheck" class="btn btn-dark  step-next-btn fw-semibold" disabled onclick="catpass(this)">Next</button>
                <button class="step-back-btn btn btn-link text-black fw-semibold" onclick="back(this)">Back</button>
            </div>
        </div>
    
        <div class="step-container d-none">
            <div class="col-lg-7 d-flex flex-column m-auto">
                <div class="p-2 mb-3 text-start">
                    <h1>Share some basics about your place</h1>
                    <p class="fw-semibold text-muted">
                        You'll add more details later, like bed types.
                    </p>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex">
                        <div class="me-auto">Guests</div>
                        <div class="p-2">
                            <button id="btn1" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">1</span>
                                <input id="geust" type="hidden" value="1" asp-for="Capacity" />
                            </div>
                        <div class="p-2">
                            <button id="btn2" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex">
                        <div class="me-auto">Bedrooms</div>
                        <div class="p-2">
                            <button id="btn3" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc1()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">1</span>
                            <input id="bedroom" type="hidden" value="1" asp-for="NumberOfBedRooms" />
                        </div>
                        <div class="p-2">
                            <button id="btn4" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown1()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex">
                        <div class="me-auto">Beds</div>
                        <div class="p-2">
                            <button id="btn5" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc2()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">1</span>
                            <input id="beds" type="hidden" value="1" asp-for="NumberOfBeds" />
                        </div>
                        <div class="p-2">
                            <button id="btn6" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown2()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                    <div class="d-flex">
                        <div class="me-auto">Bathrooms</div>
                        <div class="p-2">
                            <button id="btn7" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="Counertfunc3()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                        <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                              stroke-linejoin="round"></path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                        <div class="p-2 d-flex"><span class="p-2 counter">1</span>
                            <input id="bathrooms" type="hidden" value="1" asp-for="NumberOfBathRooms" />
                        </div>
                        <div class="p-2">
                            <button id="btn8" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                    onclick="CounertfuncDown3()">
                                <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                    <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                    <g id="SVGRepo_iconCarrier">
                                        <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                        </path>
                                    </g>
                                </svg>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                        <div class="d-flex">
                            <div class="me-auto">Minimum Stay</div>
                            <div class="p-2">
                                <button id="btn9" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                        onclick="Counertfunc4()">
                                    <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                        <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                        <g id="SVGRepo_iconCarrier">
                                            <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                            </path>
                                            <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                                  stroke-linejoin="round"></path>
                                        </g>
                                    </svg>
                                </button>
                            </div>
                            <div class="p-2 d-flex">
                                <span class="p-2 counter">1</span>
                                <input id="minstay" type="hidden" value="1" asp-for="MinStay" />
                            </div>
                            <div class="p-2">
                                <button id="btn10" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                        onclick="CounertfuncDown4()">
                                    <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                        <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                        <g id="SVGRepo_iconCarrier">
                                            <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                            </path>
                                        </g>
                                    </svg>
                                </button>
                            </div>
                        </div>
                    </div>
                <div class="d-grid gap-2 m-1 h4 pb-2 mb-4 border-bottom border-dark-subtle">
                        <div class="d-flex">
                            <div class="me-auto">Maximum Stay</div>
                            <div class="p-2">
                                <button id="btn11" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                        onclick="Counertfunc5()">
                                    <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                        <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                        <g id="SVGRepo_iconCarrier">
                                            <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                            </path>
                                            <path d="M12 5L12 19" stroke="#323232" stroke-width="2" stroke-linecap="round"
                                                  stroke-linejoin="round"></path>
                                        </g>
                                    </svg>
                                </button>
                            </div>
                            <div class="p-2 d-flex">
                                <span class="p-2 counter">2</span>
                                <input id="maxstay" type="hidden" value="2" asp-for="MaxStay" />
                            </div>
                            <div class="p-2">
                                <button id="btn12" type="button" class="btn btn-light border border-dark-subtle rounded-5"
                                        onclick="CounertfuncDown5()">
                                    <svg width="15px" height="20px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                                        <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                                        <g id="SVGRepo_iconCarrier">
                                            <path d="M5 12H19" stroke="#323232" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                            </path>
                                        </g>
                                    </svg>
                                </button>
                            </div>
                        </div>
                    </div>
            </div>
            <div class="d-flex justify-content-between container flex-row-reverse pb-5">
                    <button type="button" class="btn btn-dark float-end step-next-btn fw-semibold" onclick="GetMap()">Next</button>
                <button class="step-back-btn btn btn-link text-black fw-semibold" onclick="back(this)">Back</button>
            </div>
        </div>
        <div class="step-container d-none">
            <div class="container h-75">
                <div class="pt-3 my-2">
                    Language:
                    <select id="langOptions" onchange="updateLanguage()">
                        <option value="ar">Arabic (ar)</option>
                        <option value="en-US" selected="selected">English (United States) (en-US)</option>
                    </select>
                </div>
                <div id="myMap" class="col-md-7 m-auto" style="position:relative;aspect-ratio: 2/1;"></div>
                <div class=" container row">
                    <div class="col d-none">
                        <label for="longitude">Longitude:</label>
                        <input class="form-control" type="text" id="lon" name="longitude" value="" readonly hidden />
                    </div>
                    <div class="col d-none">
                        <label for="latitude">Latitude:</label>
                        <input class="form-control" type="text" id="lat" name="latitude" value="" readonly hidden />
                    </div>
                    <div class="col ">
                        <label for="latitude">Address:</label>
                            <input class="form-control" type="text" id="addr" name="Location" value="" readonly /></div>
                     <div class="col ">
                        <label for="latitude">Building Number:</label>
                                <input class="form-control " type="text" id="bnum" name="BuildingNumber" value="" onchange="buttondis()" disabled/></div>
                             <div class="col ">
                        <label for="latitude">Street Name:</label>
                                    <input class="form-control " type="text" id="sname" name="Street" value="" onchange="buttondis()" disabled /></div>
                    </div>
                </div>
        
            <div class="d-flex justify-content-between container flex-row-reverse pb-5">
                    <button id="mapchecking" type="button" class="btn btn-dark float-end step-next-btn fw-semibold" onclick="pass(this)" disabled>Next</button>
                <button class="step-back-btn btn btn-link text-black fw-semibold" onclick="back(this)">Back</button>
            </div>
        </div>
        <!-- step 2 -->
        <div class="step-container d-none">
            <section class="h-75 d-flex align-self-lg-center container ">
                <div class="row">
                    <div class="col-1"></div>
                    <div class="col-md-6 mt-5 d-flex align-content-center flex-column justify-content-center">
                        <p class="fw-semibold fs-4">Step 2</p>
                        <h1>Make your place stand out</h1>
                        <div class="fw-normal fs-6">
                            In this step, you’ll add some of the amenities your place offers, plus 5 or more photos. Then, you’ll create a title and description.
                        </div>
                    </div>
                    <div class="col-md-5 mt-5">

                        <video class="h-75 align-self-lg-center" autoplay>
                            <source src="./listing/Step 1- Tell us about your place.mp4" type="video/mp4">
                        </video>
                    </div>
                </div>
            </section>

            <div class="d-flex justify-content-between container flex-row-reverse pb-5">
                    <button type="button" class="btn btn-dark float-end step-next-btn fw-semibold">Next</button>
                <button class="step-back-btn btn btn-link text-black fw-semibold" onclick="back(this)">Back</button>
            </div>
        </div>
        <div class="step-container d-none">
            <section class="container">
                <div class="row d-flex justify-content-center ">
                    <div class="col-lg-7 text-start">

                        <p class="fs-2 fw-semibold mb-2">Add some photos of your house</p>
                        <p class="text-black-50">
                            You'll need 5 photos to get started. You can make changes later.
                        </p>
                        <div id="image" class="align-items-center border d-flex justify-content-center flex-column">

                            <button id="deleteimg" class="postion-edit btn btn-light rounded-4 position-absolute d-none">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" aria-hidden="true" role="presentation"
                                     focusable="false" style="display: block; height: 16px; width: 16px; fill: currentcolor;">
                                    <path d="M10 .375c.314 0 .574.231.618.533l.007.092v1.499L14 2.5v2l-1.001-.001L13 14a1 1 0 0 1-.883.993L12 15H4a1 1 0 0 1-.993-.883L3 14l-.001-9.501L2 4.5v-2l3.375-.001V1c0-.314.231-.574.533-.618L6 .375h4zM6.125 4.5h-1.25v9h1.25v-9zm2.5 0h-1.25v9h1.25v-9zm2.5 0h-1.25v9h1.25v-9zm-1.75-2.875h-2.75v.874h2.75v-.874z">
                                    </path>
                                </svg>
                            </button>
                            <img id="firstimage" src="" alt="">

                            <div id="content" class=" d-flex justify-content-center flex-column align-items-center ">

                                <svg class="text-black" viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"
